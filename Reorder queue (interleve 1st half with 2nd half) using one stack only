#include <iostream>
#include <queue>
#include <stack>
using namespace std;

int main() {
    queue<int> q;
    stack<int> st;
    int n, x;
    cin >> n;

    for(int i = 0; i < n; i++) {
        cin >> x;
        q.push(x);
    }

    // Step 1: Push first half elements in stack
    int half = n / 2;
    for(int i = 0; i < half; i++) {
        st.push(q.front());
        q.pop();
    }

    // Step 2: Push stack elements back to queue (they will be reversed)
    while(!st.empty()) {
        q.push(st.top());
        st.pop();
    }

    // Step 3: Move first half (which were originally 2nd half) to back
    for(int i = 0; i < half; i++) {
        q.push(q.front());
        q.pop();
    }

    // Step 4: Again push first half elements to stack
    for(int i = 0; i < half; i++) {
        st.push(q.front());
        q.pop();
    }

    // Step 5: Interleave stack and queue
    while(!st.empty()) {
        q.push(st.top());
        st.pop();

        q.push(q.front());
        q.pop();
    }

    // Print final queue
    while(!q.empty()) {
        cout << q.front() << " ";
        q.pop();
    }

    return 0;
}
