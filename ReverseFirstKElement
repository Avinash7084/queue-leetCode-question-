#include<iostream>
#include<stack>
#include<queue>
using namespace std;
void display(queue<int>& q){
    int m = q.size();
    for(int i=0; i<m; i++){
        int x = q.front();
        cout<<x<<" ";
        q.pop();
        q.push(x);
    }
    cout<<endl;
}

int main(){
    stack<int>st;
    queue<int>q;
    int n,k;
    cout<<"enter the number : ";
    cin>>n;
    cout<<"enter the k number : ";
    cin>>k;
    
    for(int i=1; i<=n; i++){
        q.push(i);
    }
    display(q);
    for(int i=1; i<=k; i++){
        st.push(q.front());
        q.pop();
        
    }
    while(st.size() != 0){
        q.push(st.top());
        st.pop();
    }
    for(int i=1; i<=n-k; i++){
        int x = q.front();
        q.pop();
        q.push(x);
    }
    display(q);
    return 0;
}
